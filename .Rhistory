library(DescTools)
library(stargazer)
#read in data
RegressionData = read.csv("./TableData/Regression/RegressionData.csv") %>%
filter(MV>0, Total.Assets >0)%>%
mutate(year=as.factor(year),
Industry = as.factor(Industry))
#RegressionData$RevenueGrowth = Winsorize(RegressionData$RevenueGrowth,probs=c(0,0.99))
#Revenue Growth
Reg1 = felm(RevenueGrowth~NormalizedDistance + log(MV) | year * Industry | 0 | 0 ,data=filter(RegressionData,FirmSize=="Microbusiness"))
Reg2 = felm(RevenueGrowth~NormalizedDistance + log(MV) | year * Industry | 0 | 0 ,data=filter(RegressionData,FirmSize=="Small Business"))
Reg3 = felm(RevenueGrowth~NormalizedDistance + log(MV) | year * Industry | 0 | 0 ,data=filter(RegressionData,FirmSize=="Midsize Business"))
Reg4 = felm(RevenueGrowth~NormalizedDistance + log(MV) | year * Industry | 0 | 0 ,data=filter(RegressionData,FirmSize=="Large Business"))
Reg5 = felm(RevenueGrowth~NormalizedDistance + log(MV) | year * Industry | 0 | 0 ,data=filter(RegressionData,FirmSize=="Multinational Group"))
stargazer(Reg1, Reg2, Reg3, Reg4, Reg5, title= "Regression Results",
dep.var.labels=c("Sales Growth","Tobin's Q Growth"),
add.lines = list(c("Fixed Effects","Yes","Yes")),
covariate.labels=c("Normalized Distance","log(Market Value)","log(Assets)"),
header = FALSE,
keep.stat = c("rsq","n")
)
