library(tidyverse)
library(ggplot2)
set.seed(123)
data1 <- rnorm(1000, mean = 5, sd = 2)
data2 <- rnorm(1000, mean = 7, sd = 2)
# Daten in einem DataFrame zusammenführen
df <- data.frame(
value = c(data1, data2),
group = rep(c("Gruppe 1", "Gruppe 2"), each = 1000)
)
# Histogramme erstellen und vergleichen
ggplot(df, aes(x = value, fill = group)) +
geom_histogram(position = "identity", alpha = 0.5, bins = 30) +
labs(
title = "Vergleich von zwei Histogrammen",
x = "Wert",
y = "Häufigkeit",
fill = "Gruppe"
) +
theme_minimal() +
theme(
plot.title = element_text(hjust = 0.5),
axis.title.x = element_text(size = 12),
axis.title.y = element_text(size = 12),
legend.title = element_text(size = 12),
legend.position = "top"
)
