library(ggplot2)
# Beispiel-Daten
set.seed(123)
data <- rnorm(1000, mean = 45, sd = 10)
# Daten anpassen: Werte über 50 in einer Gruppe zusammenfassen
data_adjusted <- ifelse(data > 50, 50, data)
# DataFrame erstellen
df <- data.frame(value = data_adjusted)
# Histogramm erstellen
ggplot(df, aes(x = value)) +
geom_histogram(binwidth = 5, fill = "blue", alpha = 0.7) +
scale_x_continuous(breaks = seq(0, 50, by = 5), labels = c(seq(0, 45, by = 5), "50+")) +
labs(
title = "Histogramm mit zusammengefassten Werten über 50",
x = "Wert",
y = "Häufigkeit"
) +
theme_minimal() +
theme(
plot.title = element_text(hjust = 0.5),
axis.title.x = element_text(size = 12),
axis.title.y = element_text(size = 12)
)
